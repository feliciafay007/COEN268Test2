<?xml version="1.0" encoding="utf-8"?>

<preference-headers
    xmlns:android="http://schemas.android.com/apk/res/android">

    <header
        android:fragment="test1.ui.android.felicia.preferenceactivity2.MyPreferenceFragment"
        android:icon = "@drawable/abc_ic_voice_search_api_mtrl_alpha"
        android:title="Goto: Preference fragment One"
        android:summary="An example of some first preferences.An example of some preferences.An example of some preferences." />

    <header android:fragment="test1.ui.android.felicia.preferenceactivity2.MyPreferenceFragment2"
        android:icon="@drawable/abc_ic_go_search_api_mtrl_alpha"
        android:title="Prefs 2"
        android:summary="Some other preferences you can see.">
        <!-- Arbitrary key/value pairs can be included with a header as
             arguments to its fragment. -->
        <extra android:name="someKey" android:value="someHeaderValue" />
    </header>

    <header android:icon="@drawable/abc_ic_go_search_api_mtrl_alpha"
        android:title="Intent"
        android:summary="Launches an Intent.">
        <intent android:action="android.intent.action.VIEW"
            android:data="http://www.android.com" />
    </header>
</preference-headers>